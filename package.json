{
  "name": "notifire",
  "version": "2.0.0",
  "description": "A tool for designing app-related emails, push notifications, and sms notifications using handlebars templates",
  "author": "Nick Ford <nickforddesign@gmail.com>",
  "repository": "https://github.com/nickforddesign/notifire",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "concurrently \"node build/dev-server.js\" \"node server.js\"",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "export NODE_ENV=test && jest",
    "example": "node example.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "anchorme": "^1.1.2",
    "axios": "^0.21.1",
    "body-parser": "^1.17.2",
    "brace": "^0.10.0",
    "chalk": "^2.1.0",
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "glob": "^7.1.2",
    "hogan": "^1.0.2",
    "http": "0.0.0",
    "inline-css": "^2.2.2",
    "inquirer": "^3.3.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.11",
    "postcss-js": "^1.0.0",
    "pretty-file-icons": "^2.1.2",
    "require-text": "0.0.1",
    "rimraf": "^2.6.1",
    "sendmail": "^1.2.0",
    "socket.io": "^2.0.3",
    "vee-validate": "^2.0.0-rc.14",
    "vue": "^2.3.3",
    "vue-router": "^2.6.0",
    "vue-shortkey": "^2.2.1",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "jest": "^21.0.1",
    "jest-vue-preprocessor": "^1.1.0",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^1.2.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.1.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/config",
      "<rootDir>/src/js/config"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/config",
      "<rootDir>/src/js/config"
    ],
    "moduleFileExtensions": [
      "json",
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "setupFiles": [
      "<rootDir>/src/js/config/jest.js"
    ]
  }
}
